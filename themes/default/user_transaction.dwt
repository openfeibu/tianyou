                <script>
                function update_icon(id){
                    var consignee = $(".address-item_"+id);
                    var top = $(window).scrollTop();
                    $("body").css({"overflow":"hidden"})
                    $(".update-address .address_id").val(id);
                    $(".update-address .consignee").val(consignee.find(".consignee").text());
                    $(".update-address .mobile").val(consignee.find(".mobile").text());
                    $(".update-address .dec").val(consignee.find(".address").text());
                    $(".update-address .region").val(consignee.find(".region").text());
                    $(".update-address").fadeIn(200).css({"top":top});
                }
                $("body").on("click",".update-address .close",function(){
                    $(".update-address").fadeOut(200);
                    $("body").css({"overflow":"auto"})
                })
                function addAddress(){
                    var city = $("#city").val(),
                    shopping = '{$shopping}',
                    address=$("#addAddress").find(".address").val(),
                    consignee=$("#addAddress").find(".consignee").val(),
                    mobile=$("#addAddress").find(".mobile").val();
                    if(city.split("-").length < 3){
                        $fb.fbNews({"content":"请完善你的收货地址","type":"warning"});
                    }else if(address.length <= 0){
                        $fb.fbNews({"content":"请完善你的详情地址","type":"warning"});
                    }else if(consignee.length <= 0){
                        $fb.fbNews({"content":"收货人不可为空","type":"warning"});
                    }else if(!$fb.expPhone(mobile)){
                        $fb.fbNews({"content":"手机号码格式错误","type":"warning"});
                    }else{
                        var country = $("#addAddress").find(".country").attr('data-id');
                        var province = $("#hcity").attr('data-id');
                        var city = $("#hproper").attr('data-id');
                        var district = $("#harea").attr('data-id');
                        var is_default = 0;
                        if($("#addAddress").find('.is_default').attr('checked')){
                            var is_default = 1;
                        }
                        $.ajax({
                             type: "POST",
                             url: "user.php?act=act_edit_address",
                             data: {'consignee':consignee, 'mobile':mobile,'address':address,'country':country,'province':province,'city':city,'district':district,'is_default':is_default,'shopping':shopping},
                             dataType: "json",
                             success: function(data){
                                 $fb.fbNews({"content":"增加地址成功","type":"success"});
                                 window.location.href=data.url;
                              }
                         });
                    }
                    return false;
                }
                function upAddress(address_id){
                    var city = $("#city2").val(),
                    address_id = $("#upAddress").find(".address_id").val(),
                    address=$("#upAddress").find(".address").val(),
                    consignee=$("#upAddress").find(".consignee").val(),
                    mobile=$("#upAddress").find(".mobile").val();
                    var country = $("#addAddress").find(".country").attr('data-id');
                    var province = $("#hcity").attr('data-id');
                    var city = $("#hproper").attr('data-id');
                    var district = $("#harea").attr('data-id');
                    if(typeof(province) != "undefined"){
                        if(!province || !city || !district)
                        {
                            $fb.fbNews({"content":"请完善你的收货地址","type":"warning"});
                            return false;
                        }
                    }
                    if(address.length <= 0){
                        $fb.fbNews({"content":"请完善你的详情地址","type":"warning"});
                    }else if(consignee.length <= 0){
                        $fb.fbNews({"content":"收货人不可为空","type":"warning"});
                    }else if(!$fb.expPhone(mobile)){
                        $fb.fbNews({"content":"手机号码格式错误","type":"warning"});
                    }
                    else{
                        $.ajax({
                             type: "POST",
                             url: "user.php?act=act_edit_address",
                             data: {address_id:address_id,consignee:consignee, mobile:mobile,address:address,country:country,province:province,city:city,district:district},
                             dataType: "json",
                             success: function(data){
                                 $fb.fbNews({"content":"修改地址成功","type":"success"});
                                 window.location.href=data.url;
                                 $(".update-address").fadeOut(200);
                                 $("body").css({"overflow":"auto"});
                              }
                         });
                    }
                    return false;
                }
                $("#city").click(function (e) {
