                    }else if(user_name.length <= 0){
                        $fb.fbNews({"content":"昵称不能为空","type":"warning"});
                    }else if(!sex){
                        $fb.fbNews({"content":"请选择性别","type":"warning"});
                    }else{
                        var country = $(".personal").find(".country").attr('data-id');
                        var province = $(".personal").find("#hcity").attr('data-id');
                        var city = $(".personal").find("#hproper").attr('data-id');
                        var district = $(".personal").find("#harea").attr('data-id');
                        var birthdayYear = $(".personal").find('select[name="birthdayYear"]').val();
                        var birthdayMonth = $(".personal").find('select[name="birthdayMonth"]').val();
                        var birthdayDay = $(".personal").find('select[name="birthdayDay"]').val();
                        $.ajax({
                             type: "POST",
                             url: "user.php?act=act_edit_profile",
                             data: {user_name:user_name,sex:sex,country:country,province:province,city:city,district:district,birthdayYear:birthdayYear,birthdayMonth:birthdayMonth,birthdayDay:birthdayDay},
                             dataType: "json",
                             success: function(data){
                                 if(data.error == 403)
                                 {
                                     window.location.href='user.php';
                                 }else if(data.error == 0){
                                     $fb.fbNews({"content":data.message,"type":"success"});
                                 }else{
                                     $fb.fbNews({"content":data.message,"type":"warning"});
                                 }

                              }
                         });
                    }
                    return false;
                }
                </script>
